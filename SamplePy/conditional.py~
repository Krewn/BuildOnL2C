t = True
f = False

if(t):
	print("If yes, yeah.")

if(f):
	print("If no, no.")
else:
	print("If no, else...")

if(t or f): # In java the or operation is made as shown.
	print("T or F -> T")

if(t and f):
	print("T and F -> F")
else:
	print("T and F -> F")

#We can also use the not operation !
if(not(f)):
	print("not(F) -> T")

#a few more examples
if( not(f or f) and t):
	print("not(F or F) and T -> T")

if( (t or f) or not(t and t)):
	print("(T or F) or not(T and T) -> T")
